<?xml version="1.0" encoding="utf-8"?>

	<selector xmlns:android="http://schemas.android.com/apk/res/android">
		<item android:state_pressed="true" > <!-- pressed -->
			<shape android:shape="rectangle">
				<!-- solid android:color="#8e6500"/ -->
			    <solid android:color="@color/dialog_select"/>
			</shape>
		</item>
	    <item android:state_focused="true" > <!-- focused -->
			<shape android:shape="rectangle">
				<solid android:color="@color/dialog_select"/>
			</shape>
		</item>
		<item > <!-- default -->
			<shape android:shape="rectangle">
				<solid android:color="@color/dialog_bg"/>
			</shape>
		</item>
	</selector>
